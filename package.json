{
  "name": "create-deskthing",
  "private": false,
  "version": "0.10.2",
  "version_code": 10,
  "type": "module",
  "description": "The DeskThing template for building DeskThing Apps",
  "main": "scripts/dist/index.js",
  "dependencies": {
    "chalk": "^5.3.0",
    "ejs": "^3.1.10",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1"
  },
  "bin": {
